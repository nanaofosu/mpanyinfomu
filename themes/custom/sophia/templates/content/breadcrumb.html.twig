{#
/**
 * @file
 * Default theme implementation for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 *
 * @ingroup themeable
 */
#}
{% for item in breadcrumb %}
  {% if 'Symptom Association'  in item.text %}
    <span style="display: none;" id="symptom-association-topic">{{item.text | replace({'Symptom Association » Group » Title: ': ''}) |title }}</span>
  {% endif %}
{% endfor %}



{% if breadcrumb %}
  <nav role="navigation" aria-labelledby="system-breadcrumb">
    
    <h2 id="system-breadcrumb" class="visually-hidden">{{ 'Breadcrumb'|t }}</h2>
    <ol>
    {% for item in breadcrumb %}
      <li>
        {% if item.url %}
          <a href="{{ item.url }}">{{ item.text }}</a>
        {% else %}
          {{ item.text }}
        {% endif %}
      </li>
    {% endfor %}
    </ol>
  </nav>
{% endif %}